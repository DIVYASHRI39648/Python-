# PROGRAM ON THE NUMBER DIVISIBLE BY 3 

 def result(N): 

       

     # iterate from 0 to N 

     for num in range(N): 

           

             # Short-circuit operator is used 

             if num % 3 == 0 and num % 5 == 0: 

                 print(str(num) + " ", end = "") 

                   

             else: 

                 pass
